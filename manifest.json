{
  "name": "GPM (German Pronounce & Meaning)",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["german_dict.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
